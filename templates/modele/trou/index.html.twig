  {% block script %}
  {% endblock %}
  <script type="text/javascript">
    
    var d = document.getElementById('dash');
    var prints = document.getElementById('print');

     prints.onclick = function(){
      d.innerHTML = '';

      window.print();

      return false;
     };

     var temps = 4;
     var compt = setInterval(function(){
        --temps;
        if (temps == 3) { prints.style.background = 'red'; prints.style.color='yellow'; }
        if (temps == 2) { prints.style.background = 'yellow'; prints.style.color='white'; }
        if (temps == 1) { prints.style.background = 'green'; prints.style.color='white'; temps = 4; }
     },1000);

